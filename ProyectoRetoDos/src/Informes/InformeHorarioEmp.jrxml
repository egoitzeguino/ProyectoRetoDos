	<parameter name="dni" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT 
tortugajuanes.empleado.nombre_emp,
 tortugajuanes.empleado.apel_emp,
  tortugajuanes.empleado.antiguedad, 
  tortugajuanes.empleado.salario, 
  tortugajuanes.fichaje.horario_ent, 
  tortugajuanes.fichaje.horario_sal, 
  tortugajuanes.fichaje.total_horas, 
  SUM(tortugajuanes.fichaje.total_horas) 
  OVER (PARTITION BY tortugajuanes.empleado.dni) as total_horas_suma 
  FROM 
  tortugajuanes.empleado 
  INNER JOIN tortugajuanes.fichaje ON tortugajuanes.empleado.dni = tortugajuanes.fichaje.dni 
  WHERE 
  tortugajuanes.empleado.dni =  $P{dni}]]>
